isKdmEffectsObject = true
effects = {
    strength = 1
}

function getEffects()
    rot = self.getRotation()
    if rot[3] > 345 or rot[3] < 15 then
        effects.strength = 1
    else
        effects.strength = -1
    end
	local qty = self.getQuantity()
	if qty >= 2 then
		effects.strength = effects.strength * qty
	end
    return effects
end
