isKdmEffectsObject = true
effects = {
    evasion = 1
}

function getEffects()
    rot = self.getRotation()
    if rot[3] > 345 or rot[3] < 15 then
        effects.evasion = 1
    else
        effects.evasion = -1
    end
	local qty = self.getQuantity()
	if qty >= 2 then
		effects.evasion = effects.evasion * qty
	end
    return effects
end