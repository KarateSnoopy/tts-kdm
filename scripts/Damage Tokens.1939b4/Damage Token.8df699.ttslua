isKdmEffectsObject = true
effects = {
    damage = 1
}

function getEffects()
    rot = self.getRotation()
    if rot[3] > 345 or rot[3] < 15 then
        effects.damage = 1
    else
        effects.damage = -1
    end
	local qty = self.getQuantity()
	if qty >= 2 then
		effects.damage = effects.damage * qty
	end
    return effects
end